chromosome MyNSGA::tournamentSelection() {
	chromosome cr;
		return cr;
}

void MyNSGA::nsga2(population *p)
{
	int NP = p->get_size();
	std::vector<int> v1(NP), v2(NP);
	for(int i=0;i < NP; i++)
	{
		v1[i] = i;
		v2[i] = i;
	}
	for(int g =0; g< this->nb_gen; g++)
	{
		p->update_pareto_information();
		std::vector<chromosome> cr = p->getIndividuals();
	}
}
