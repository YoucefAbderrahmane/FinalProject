#include "gene.h"

gene::gene() : observation(), random_selection(), start_date(), end_date(), telescope_used() {
	// TODO Auto-generated constructor stub

}

gene::gene(Observation * observation) : random_selection(), start_date(),
		end_date(), telescope_used(){

	this->observation = observation;
}

gene::~gene() {
	// TODO Auto-generated destructor stub
}

bool gene::is_scheduled() {

	std::cout << random_selection << " " << observation->getRequest()->getPriority() << std::endl;
	return ( random_selection < observation->getRequest()->getPriority());
}

Observation* gene::getObs() {

	return observation;
}
